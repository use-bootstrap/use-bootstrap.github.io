{
  "name": "use-bootstrap",
  "description": "Bootstrap library collection",
  "version": "0.0.0",
  "repository": "https://github.com/use-bootstrap",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "bootstrap5",
    "bootstrap libraries",
    "bootstrap5 libraries",
    "bootstrap dialog",
    "bootstrap alert",
    "bootstrap confirm",
    "bootstrap prompt",
    "bootstrap select",
    "bootstrap dropdown",
    "bootstrap tag",
    "bootstrap tag input",
    "bootstrap toast"
  ],
  "scripts": {
    "dev": "halaman dev src/pages --watch=src/**/*",
    "build": "halaman build src/pages docs --pretty && purgecss --css docs/bootstrap.min.css --content docs/index.html --output docs/bootstrap.min.css && halaman serve docs",
    "serve": "halaman serve docs"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.41.0",
    "@types/node": "20.5.6",
    "@types/react": "18.2.21",
    "eslint": "8.47.0",
    "eslint-plugin-react": "7.33.2",
    "halaman": "0.2.4",
    "purgecss": "5.0.0",
    "react": "18.2.0",
    "react-icons": "4.10.1",
    "typescript": "5.2.2"
  }
}
